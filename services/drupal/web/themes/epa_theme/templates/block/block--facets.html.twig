{#
 /**
  * @file
  * Theme override to display a block.
  */
#}

{% embed '@components/block/block.twig' with {
  'hide_wrapper': not logged_in,
  'has_constrain': false,
  'title_prefix': title_prefix,
  'title_suffix': title_suffix,
  'label': false,
} %}

  {% block content %}
    {{ content }}
  {% endblock %}

{% endembed %}
