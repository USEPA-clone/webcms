{#
/**
 * @file
 * Theme override for a paragraph block.
 */
#}
{% set block_content %}
  {% block block_content %}
    {{ content }}
  {% endblock %}
{% endset %}

{% embed '@components/block/block.twig' with {
  'hide_wrapper': not logged_in,
  'has_constrain': false,
  'title_prefix': title_prefix,
  'label': label,
  'title_suffix': title_suffix,
  'hide_content_wrapper': true,
} %}

  {% block content %}
    {{ block_content }}
  {% endblock %}

{% endembed %}
